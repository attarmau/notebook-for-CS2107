import sys

def power_modulo(m, k, n):
    if k == 0:
        return 1 % n
    elif k % 2 == 0:
        half_result = power_modulo(m, k // 2, n)
        return (half_result * half_result) % n
    else:
        return (m * power_modulo(m, k - 1, n)) % n
    
num_line = int(sys.stdin.readline()) # automatically run the dataset
for _ in range(num_line):
    a = [int(s) for s in sys.stdin.readline().split()]
    m, k, n = a[0], a[1], a[2]
    print(power_modulo(m, k, n))
